            "apiVersion": "2016-03-30",
            "location": "japanwest",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.3.0.5",
                            "privateIPAllocationMethod": "Dynamic",
                            "subnet": {
